@article{moosmullerStandardAustrianGerman2015,
  title = {Standard {{Austrian German}}},
  author = {Moosmüller, Sylvia and Schmid, Carolin and Brandstätter, Julia},
  date = {2015-12},
  journaltitle = {Journal of the International Phonetic Association},
  shortjournal = {Journal of the International Phonetic Association},
  volume = {45},
