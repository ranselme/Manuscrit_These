@article{howsonRhoticsPalatalizationAcoustic2018,
  title = {Rhotics and {{Palatalization}}: {{An Acoustic Examination}} of {{Upper}} and {{Lower Sorbian}}},
  shorttitle = {Rhotics and {{Palatalization}}},
  author = {Howson, Phil},
  date = {2018},
  journaltitle = {Phonetica},
  shortjournal = {PHO},
  volume = {75},
  number = {2},
  eprint = {29433119},
