  author = {Alfwaress, Firas and Maaitah, Emad Al and Al-Khateeb, Suzan and Zama, Zakaria Abu},
  date = {2015},
  journaltitle = {International Journal of Speech-Language Pathology},
  shortjournal = {International Journal of Speech-Language Pathology},
  volume = {17},
  number = {5},
  pages = {518--526},
  issn = {1754-9507, 1754-9515},
  doi = {10.3109/17549507.2015.1024165},
  url = {http://www.tandfonline.com/doi/full/10.3109/17549507.2015.1024165},
  urldate = {2020-06-22},
  abstract = {Purpose: To investigate the relationship of the vocal tract dimensions and dental arches in participants with substitution of palatal approximant /j/ for the trill variant of /r/. Method: Cephalograms and dental casts of 60 native speakers of Arabic were used for comparison. The experimental group included 30 participants with substitution of /j/ for the trill variant of /r/ (17 males and 13 females, age ranged between 15–20 years, M ϭ 18.7 years, SD ϭ 1.27). The control group included 30 normal speakers (15 males and 15 females, age ranged between 15–20 years, M ϭ 18.3 years, SD ϭ 1.67). Both groups had class I occlusion. Result:The experimental group showed significantly shorter maxillary length, p Ͻ 0.05, and narrower vocal tract dimensions compared to the control group, p Ͻ 0.05.Tongue length (TGL) and height (TGH) and soft palate length (PNSP) and thickness (MPT) were statistically significantly reduced in the experimental group compared to the control group, p Ͻ 0.05. The distance between the hyoid bone and retrognathion or C3 was significantly shorter in the experimental group compared to the control group, p Ͻ 0.05. Inter-canine widths and dental arch lengths were significantly shorter in the experimental group compared to the control group, p Ͻ 0.05. Conclusion: The substitution of /j/ for the trill variant of /r/ is associated with altered dimensions of the vocal tract. These alterations may explain the residual articulation disorder during the production of the trilled /r/ compared to other sound classes that respond to speech therapy. Orthodontic and Maxillo facial surgery consultation might be needed when providing therapy to individuals with this particular articulation disorder.},
  langid = {english},
  keywords = {arabic,clinical,disorders,individual variation,lu - read,palatal,palatalization,paper version,rhotics,substitution,trill,vocal tract},
  file = {/home/remianselme/ZoteroMAC/storage/MZW68LEZ/Alfwaress et al. - 2015 - The relationship of vocal tract dimensions and sub.pdf}
}

