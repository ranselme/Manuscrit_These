@thesis{jauriberryRhotiquesRhoticiteEcosse2016,
  type = {phdthesis},
  title = {Rhotiques et rhoticité en Écosse : une étude sociophonétique de l'anglais écossais standard},
  shorttitle = {Rhotiques et rhoticité en Écosse},
  author = {Jauriberry, Thomas},
