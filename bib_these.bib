@article{renwickPhoneticDistinctivenessVs2016,
  title = {Phonetic {{Distinctiveness}} vs. {{Lexical Contrastiveness}} in {{Non-Robust Phonemic Contrasts}}},
  author = {Renwick, Margaret and Ladd, D.},
  date = {2016-12-28},
  journaltitle = {Laboratory Phonology: Journal of the Association for Laboratory Phonology},
  volume = {7},
  number = {1},
  pages = {19},
  publisher = {{Ubiquity Press}},
  issn = {1868-6354},
  doi = {10.5334/labphon.17},
  url = {http://www.journal-labphon.org/articles/10.5334/labphon.17/},
  urldate = {2021-09-02},
  abstract = {It is known that the mid vowel contrasts of Standard Italian distinguish few minimal pairs, may be lexically variable, and show some degree of phonological conditioning in certain varieties. As such, they are relevant to recent suggestions that phonemic contrast may be partial, gradient, or otherwise more cognitively complex than traditionally assumed. Production data and vowel height judgments from 17 speakers con rm that most have clear phonetic distinctions between higher and lower mid vowels. However, the lexical distribution of these vowels is variable, and (in some speakers) phonologically conditioned to some extent; and though phonological awareness for all speakers is broadly accurate, we also observe cases where production and speaker judgment fail to match, in part because individual speakers’ productions are variable. This suggests that the somewhat marginal status of the Italian mid vowel contrasts resides in the link between phonetic categories and individual lexical items, not in any indistinctness of the phonetic categories themselves.},
  issue = {1},
  langid = {english},
  keywords = {à lire - to read,contrast,experimental phonetics,individual variation,italian,marginal contrast,phonology,vowels,written english},
  file = {/home/remianselme/ZoteroMAC/storage/YJ9ZYQ4A/Renwick et Ladd - 2016 - Phonetic Distinctiveness vs. Lexical Contrastivene.pdf;/home/remianselme/ZoteroMAC/storage/YFQNAGXR/labphon.17.html}
}

