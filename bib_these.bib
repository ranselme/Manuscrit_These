  shorttitle = {Children's {{Consonant Acquisition}} in 27 {{Languages}}},
  author = {McLeod, Sharynne and Crowe, Kathryn},
  date = {2018-11-21},
  journaltitle = {American Journal of Speech-Language Pathology},
  shortjournal = {Am J Speech Lang Pathol},
  volume = {27},
