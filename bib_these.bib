@thesis{inouyeTrillsTapsStops1995,
  type = {phdthesis},
  title = {Trills, Taps, and Stops in Contrast and Variation},
  author = {Inouye, Susan Banner},
  date = {1995},
  journaltitle = {ProQuest Dissertations and Theses},
  institution = {{University of California}},
  location = {{Los Angeles}},
  url = {https://www.proquest.com/docview/304186474/abstract/2B84634C84BD4442PQ/1},
  urldate = {2022-10-26},
  abstract = {In the world's languages there is a close relationship among three lingual segment types: taps/flaps such as (c), trills such as (r), and stops such as (d). These segment types are phonetically similar, all involving contact between the tongue tip/blade and the region on/near the alveolar ridge. This dissertation is a typological survey of the phonetic and phonological relationships among these sounds using data from about 65 languages from a broad range of language families, including original phonetic data from about 15 languages. Several case studies are presented which demonstrate that intervocalic tapping of stops is widespread crosslinguistically. Phonetic data show that intervocalic tapping of trills is also widespread. These processes are analyzed as lenition in the temporal and spatial domains. A representation depicting this lenition process as the spreading of aperture is proposed. It is also shown that not all trills and taps that contrast in a language can be represented as a geminate/singleton pair, so an additional feature is needed to represent this contrast. Incorporating the generalization about the phonetic relationship between trills and taps, a phonetic length feature is proposed. In addition, the allophonic interaction between stops and trills in examined, revealing that in most cases an intermediate step of tap can be found. In three cases, the trill/stop alternation is phonologized to the extent that the intermediate tap is no longer evident. Also several languages with prestopped trills are examined, leading to the conclusion that they are trills that begin at the closure phase of the vibratory cycle and need no special representation.},
  isbn = {9798209289791},
  langid = {english},
  keywords = {Language,lenition,literature and linguistics,phonetics,phonology},
  file = {/home/remianselme/ZoteroMAC/storage/DRLQFEW3/Inouye - 1995 - Trills, taps, and stops in contrast and variation.pdf}
}

