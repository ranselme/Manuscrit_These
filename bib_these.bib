@article{spajicTrillsToda1996,
  title = {The Trills of {{Toda}}},
  author = {Spajić, Siniša and Ladefoged, Peter and Bhaskararao, P.},
  date = {1996-06},
  journaltitle = {Journal of the International Phonetic Association},
  shortjournal = {Journal of the International Phonetic Association},
  volume = {26},
