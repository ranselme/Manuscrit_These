@article{burrowsAllophonyEnglishLanguage2019,
  title = {Allophony in {{English Language Learners}}: {{The Case}} of {{Tap}} in {{English}} and {{Spanish}}},
  shorttitle = {Allophony in {{English Language Learners}}},
  author = {Burrows, Lauren and Jarmulowicz, Linda and Oller, D. Kimbrough},
  date = {2019-01-28},
  journaltitle = {Language, Speech, and Hearing Services in Schools},
  shortjournal = {LSHSS},
  volume = {50},
  number = {1},
  pages = {138--149},
