@incollection{maddiesonPhoneticFieldwork2001,
  title = {Phonetic Fieldwork},
  booktitle = {Linguistic {{Fieldwork}}},
  author = {Maddieson, Ian},
  editor = {Ratliff, Martha and Newman, Paul},
  date = {2001},
  pages = {211--229},
  publisher = {{Cambridge University Press}},
