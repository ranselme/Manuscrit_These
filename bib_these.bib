@article{pittsInfluenceOropalatalDimensions2017,
  title = {The {{Influence}} of {{Oropalatal Dimensions}} on the {{Measurement}} of {{Tongue Strength}}},
  author = {Pitts, Laura L. and Stierwalt, Julie A. G. and Hageman, Carlin F. and LaPointe, Leonard L.},
  date = {2017-12},
  journaltitle = {Dysphagia},
  shortjournal = {Dysphagia},
  volume = {32},
  number = {6},
