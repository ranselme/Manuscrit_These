  title = {Nasal Harmony in {{Ikwere}}, a Language with No Phonemic Nasal Consonants},
  author = {Clements, G. N. and Osu, Sylvester},
  date = {2005-12-12},
  journaltitle = {Journal of African Languages and Linguistics},
  volume = {26},
  number = {2},
  pages = {165--200},
