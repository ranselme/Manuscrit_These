  keywords = {dialect,gender,gestures,individual variation,lu - read,sociophonetics,spanish,trill,variation,vowel,written english},
  file = {/home/remianselme/ZoteroMAC/storage/VYHHSNJN/Henriksen - 2014 - Sociophonetic analysis of phonemic trill variation.pdf}
}

