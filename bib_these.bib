@thesis{etchebestVariationSociophonetiqueVibrantes2017,
  type = {These de doctorat},
  title = {La Variation Socio-Phonétique Des Vibrantes de {{Soule}}},
  author = {Etchebest, Xantiana},
  date = {2017-12-07},
  institution = {{Pau}},
  url = {https://www.theses.fr/2017PAUU1030},
  urldate = {2021-07-16},
  abstract = {Ce travail montre la variation socio-phonétique de la vibrante /r/ dans la province basque de Soule. La prononciation de cette consonne est en constante évolution et le dialecte souletin ne fait pas exception. Cette évolution a été étudiée suivant la variation diastratique qui nous amène à étudier les changements par le biais de facteurs sociaux.En basque et ici en souletin, il y a deux différentes vibrantes : d’un côté il y a la vibrante battue /ɾ/ et de l’autre la roulée /r/. La première vibrante ne se prononce généralement pas entre deux voyelles en souletin. La seconde en revanche se prononce dans tous les contextes.  Mais ces vibrantes connaissent à ce jour deux formes de prononciation à savoir la forme apicale (/r/) et la forme uvulaire (/ʀ/). C’est pour cela que différentes options s’offrent à chaque interlocuteur souletin. Nous verrons dans ce travail que les choix ce font par le biais des facteurs sociaux. Les jeunes ont tendance à utiliser un son plutôt qu’un autre et les plus âgés en utilisent d’autres.Les données ont été recueillies grâce à des interviews effectuées sur le terrain grâce à un questionnaire basé sur la traduction de mots. Il leur a été demandé de traduire les même mots afin d’avoir une base de donnée qui permettrait par la suite de comparer les vibrantes dans des contextes différents. Les interlocuteurs ont été choisis suivant leur âge (jeunes, adultes, personnes plus âgées), leur sexe (homme ou femme), l’origine de leur basque (si ils l’ont appris à la maison ou à l’école), la langue dans laquelle ils travaillent (en basque ou en français) et si ils ont quitté le territoire du Pays Basque plus de 4 mois ou non.Ces données ont ensuite été analysées principalement grâce à deux programmes qui sont Praat et SPSS. C’est avec ces résultats recueillis que des recherches statistiques ont été menées, et que nous avons pu en ressortir l’état actuel des vibrantes en Soule.},
  editora = {Videgain, Charles and Gaminde, Iñaki},
  editoratype = {collaborator},
  keywords = {acoustics,basque,phonetics,rhotic,trill,variation,written basque},
  file = {/home/remianselme/ZoteroMAC/storage/FMH2PH79/Etchebest - 2017 - La variation socio-phonétique des vibrantes de Sou.pdf}
}

